#include<stdio.h>
#include<string.h>
void main()
{
  int k=0;
  char s[100];b[100];
  printf("Enter the string");
  scanf("%s",s);
  for(int i=0;s[i]!='/0';i++)
  {
    if((to lower(s[i]>97)&&tolower(s[i]<=122)&&tolower(s[i]!='e')&&tolower(s[i]!=i)&&tolower(s[i]!=o)&&tolower(s[i]!=u));
    {
    b[k++]=s[i];
    }
    }
    printf("%s",b);
    }
 
